{
  "name": "appworks-cli",
  "description": "Command line tools for OpenText AppWorks",
  "version": "1.0.0",
  "author": "Jason Ibrahim",
  "bin": {
    "appworks": "./bin/run"
  },
  "bugs": "https://github.com/opentext/appworks-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "@types/archiver": "^2.1.2",
    "@types/ora": "^1.3.4",
    "archiver": "^3.0.0",
    "cli-ux": "^4.9.1",
    "nodegit": "^0.22.2",
    "ora": "^3.0.0",
    "properties-reader": "^0.0.16",
    "rimraf": "^2.6.2",
    "tslib": "^1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/mocha": "^5",
    "@types/node": "^10",
    "@types/nodegit": "^0.22.3",
    "@types/properties-reader": "^0.0.1",
    "@types/rimraf": "^2.0.2",
    "@types/uuid": "^3.4.4",
    "chai": "^4",
    "globby": "^8",
    "mocha": "^5",
    "nyc": "^13",
    "ts-node": "^7",
    "tslint": "^5",
    "typescript": "^3.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/opentext/appworks-cli",
  "keywords": [
    "oclif"
  ],
  "license": "Apache 2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "appworks",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "opentext/appworks-cli",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rimraf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
